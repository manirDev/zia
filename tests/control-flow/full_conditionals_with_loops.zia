// @tag conditional, loop
// @description All conditional branches with their own loops

var flag = 3;

si (flag == 1) {
  pour (var i = 0; i < 1; i = i + 1) {
    afficher "One";
  }
} sinon si (flag == 2) {
  var i = 0;
  tantque (i < 1) {
    afficher "Two";
    i = i + 1;
  }
} sinon {
  afficher "Other";
  pour (var j = 0; j < 2; j++) {
    afficher j;
  }
}

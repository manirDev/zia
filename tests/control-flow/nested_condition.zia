// @author Manir
// @description Nested `si` blocks
// @tag condition
// @importance 3
var note = 75;

si (note >= 50) {
    afficher "réussi";
    si (note< 70) {
        afficher "mention";
    }
    sinon si(note > 80 et note < 90){
        afficher "mention bien";
    }
    sinon {
        afficher "mention passable";
    }

    afficher "Congrats";
}
// expect: réussi
// expect: mention
